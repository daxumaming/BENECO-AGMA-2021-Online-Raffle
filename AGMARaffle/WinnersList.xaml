<Window x:Class="AGMARaffle.WinnersList"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AGMARaffle"
        mc:Ignorable="d"
        Title="List of ARGAM 2022 Raffle Winners" Height="450" Width="950">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <DataGrid x:Name="listWinners" AlternatingRowBackground="#FFF0F0F0" FontSize="20" ItemsSource="{Binding Path=LoadDataBinding}" >
        </DataGrid>
        
    </Grid>
</Window>
